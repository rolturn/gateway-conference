$tablet-shift: 0;
$v-tablet-shift: 250;


@mixin slot($top:0,$left:0,$width:50%,$height:265px,$mobile:0) {
	position: if($mobile == 1, static, absolute);
	left: if($mobile == 1, 0, #{$left});
	top: if($mobile == 1, 0, #{$top});
	a.mosaic-element {
		width: if($mobile == 1, 100%, #{$width});
		height: if($mobile == 1, 265px, #{$height});
	}
}

.view-stories-mosaic {
	.view-content{
		position: relative;
		height: 555px;
		@include break(mobile) { 
			height: auto;
		}
		.views-row {
			position: absolute;
			left: 0;
			top: 0;
			@include transitions();
			&.views-row-1 {
				@include slot($slot1_top,$slot1_left,$slot1_width,$slot1_height);
				@include break(tablet) {
					@include slot($slot1_top,$slot1_left+$tablet-shift,$slot1_width,$slot1_height); 
				}
				@include break(v-tablet) {
					@include slot($slot2_top,$slot2_left - $v-tablet-shift,$slot2_width,$slot2_height); 
				}
				@include break(mobile) { 
					@include slot($slot2_top,$slot2_left,$slot2_width,$slot2_height,1); 
				}
			}
			&.views-row-2 {
				@include slot($slot2_top,$slot2_left,$slot2_width,$slot2_height);
				@include break(tablet) {
					@include slot($slot2_top,$slot2_left+$tablet-shift,$slot2_width,$slot2_height); 
				}
				@include break(v-tablet) { 
					@include slot($slot3_top,$slot3_left - $v-tablet-shift,$slot3_width+20px,$slot3_height); 
				}
				@include break(mobile) { 
					@include slot($slot2_top,$slot2_left,$slot2_width,$slot2_height,1); 
				}
			}
			&.views-row-3 {
				@include slot($slot3_top,$slot3_left,$slot3_width,$slot3_height);
				@include break(tablet) {
					@include slot($slot3_top,$slot3_left+$tablet-shift,$slot3_width,$slot3_height); 
				}
				@include break(v-tablet) { 
					@include slot($slot5_top,$slot5_left - $v-tablet-shift - 100px,$slot5_width+100px,$slot5_height); 
				}
				@include break(mobile) { 
					display: none;
				}
			}
			&.views-row-4 {
				@include slot($slot4_top,$slot4_left,$slot4_width,$slot4_height);
				@include break(tablet) { 
					@include slot($slot5_top,$slot5_left+$tablet-shift,$slot5_width,$slot5_height); 
				}
				@include break(v-tablet) { 
					@include slot($slot6_top,$slot6_left - $v-tablet-shift,$slot6_width,$slot6_height); 
				}
				@include break(mobile) { 
					display: none;
				}
			}
			&.views-row-5 {
				@include slot($slot5_top,$slot5_left,$slot5_width,$slot5_height);
				@include break(tablet) { 
					@include slot($slot6_top,$slot6_left+$tablet-shift,$slot6_width,$slot6_height); 
				}
				@include break(v-tablet) { display: none; }
			}
			&.views-row-6 {
				@include slot($slot6_top,$slot6_left,$slot6_width,$slot6_height);
				@include break(tablet) { display: none; }
			}
		}
	}
}


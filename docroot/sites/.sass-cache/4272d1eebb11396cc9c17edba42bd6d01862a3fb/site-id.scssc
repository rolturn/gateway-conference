	}
	h2 {
		font-size: 1.5em;
		padding: 0;
		color: $yellow;
	}
	h4 {
		font-size: 1em;
		padding: 10px 0;
	}
	p {
		padding: 15px 0;
	}
	
	em {
		font-size: .7em;
		text-transform: uppercase;
		font-style: normal;
		font-weight: 600;
	}
	
	.resource_file {
		padding: 10px 0;
	}
	
	.resource_audio {
		padding: 10px 0;
	}
	
	.content {
		padding: 15px 0;
	}
	
	.button {
		width: 90%;
		font-weight: 800;
	}
}


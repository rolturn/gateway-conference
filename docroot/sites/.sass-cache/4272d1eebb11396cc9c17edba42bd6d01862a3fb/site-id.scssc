// !Grid Views =======================================================

.grid-container {
	float: left;
	clear: none;
	
	a.grid-element {
		&.event, &.story {
			display: block;
			height:300px;
			overflow:hidden;
			margin: 0 1px 1px 0;
			position: relative;
			
			h3.title {
				color: $black;
				font-size: 1.3em;
				max-height: 65px; // We're limiting the title height in case they use long titles.
				overflow: hidden;
			}
			
			&:hover, &:active {
				h3.title { color: $yellow; }
				.img-fill {
					@include grayscale(0);
				}
	
				.img-mask { 
					@include trans-bg($light-blue,0);
				}
				
				.label {
					border-radius: 0;
					min-height: 226px;
					@include trans-bg($dark-blue,.6);
				}
				
				p.caption {
					max-height: 300px;
					opacity: 1;
					margin: 10px 0 0 0;
					//@include transitions();
				}
				
				.button {
					max-height: none;
					opacity: 1;
					&:hover {
						width: 55%;
					}
				}
		    }
		   
		    .label {
		    	display: block;
		    	width: 50%;
		    	padding: 17px;
		    	@include trans-bg(darken($yellow,5%),.8);
		    	position: relative;
		    	z-index: 100;
		    	min-height: 0px;
		    	@include border-radius(0,0,$b-radius,0);
		    	@include transitions();
			}
			     
		    .label small { padding-bottom: 20px; }
		    
		    .date {
			    text-shadow: 0px 0px 5px darken($yellow,40%);
		    }
		    strong {}

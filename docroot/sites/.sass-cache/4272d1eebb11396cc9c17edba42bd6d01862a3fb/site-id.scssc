// !Page Intro Styling =======================================================

.page-intro {
	font-size: 1em;
	line-height: 1.8em;
	max-height: 180px;
	overflow: hidden;
	margin: 0 $wrap-pad 1em $wrap-pad;
	padding: 20px $wrap-pad;
	@include transitions('1000ms');
	text-align: center;
	@include border-radius($b-radius,$b-radius,$b-radius,$b-radius);
	@include trans-bg($black,0.4);
	position: relative;
	cursor:  pointer;
	
	&:after {
		content: " ";
		width: 32px;
		height: 32px;
		position: absolute;
		margin: 0 50%;
		display: block;
		bottom: 5px;
		left: 0;
		background-image: url(../images/arrows.png);
		background-repeat: no-repeat;
		opacity: .7;
		@include transitions();
		background-position: center -32px;
	}
		
	&.active:after {
		@include transitions();
		//display: none;
		background-position: center 0px;
	}	
		
	@include break(tablet) {
		max-height: 160px;					
	}
	
	@include break(mobile) {
		max-height: 2000px;
	}
	
	&.active {
		@include trans-bg($black,0.7);
		.page-summary {
			max-height: 1000px;
			opacity: 1;
			padding: 15px 0;
		}
		h2.bi-line {
			color: $yellow;
			padding: 0;
			margin: 0 $wrap-pad 0 $wrap-pad;
		}
	}
		
	h2.bi-line {
		text-shadow: 1px 1px 7px $black;
		font-size: 2.5em;
		margin: 0 $wrap-pad .5em $wrap-pad;
		line-height: 1;
		display: block;
		font-weight: 100;
		position: relative;
		p {
			line-height: 1;
		}
		@include break(mobile) {
			margin: .5em 5% 0;
			text-align: left;
		}
	}
	
	.page-summary {
		max-height: 0;
		opacity: 0;
		text-align: left;
		@include transitions('1000ms');
		&.scripture {
			text-align: center;
		}
		
		em.scripture-ref {
			color: $yellow;
			padding: .25em 0 1em 0;
			display: block;
		}
		
		&.col {
			@include multi-col-text(3,20px);
			
			@include break(tablet) {
				@include multi-col-text(2,20px);
				margin: 0 5%;
			}
			
			@include break(mobile) {
				@include multi-col-text(1,0);
				margin: 0 5%;
			}
		}
	}
	
	&.active {
		max-height: 2000px;
	}
	
	@include break(mobile) { 
		margin: 0;
		h2.bi-line {
			font-size: 2em;
			margin: 0 $wrap-pad 0 $wrap-pad;
		}
		.page-summary {
			&.scripture {
				text-align: left;
			}
		}
	}
}




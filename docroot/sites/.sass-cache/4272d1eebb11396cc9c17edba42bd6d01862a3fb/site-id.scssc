.arrow {
	position: relative;
	@include transitions();
		
	&:after {
		content: " ";
		display: block;
		width: 32px;
		height: 32px;
		position: absolute;
		background-image: url(../images/arrows.png);
		background-repeat: no-repeat;
		opacity: .7;
		@include transitions();
		top: -6px;
		right: -40px;
	}
	
	&.right:after {
		background-position: left -96px;
	}
	
	&.left:after {
		background-position: left -64px;
		top: -6px;
		left: -40px;
	}
	
	&.up:after {
		background-position: left 0px;
	}
	
	&.down:after {
		background-position: left -32px;
	}
}
.button:hover .arrow.right:after {
	right: -70px;
	opacity: 0;
}
.button:hover .arrow.down:after {
	top: 8px;
	opacity: 0;
}


.large-ribbon {
	overflow: visible;
	position: relative;
	z-index: 300;
	width: 42%;

	> span {
		padding: 30px 8%;
		display: block;
		position: relative;
		@include trans-bg($light-blue,.8);
		@include outer-glow(20,5);
	}	

	&.yellow {
		> span {
			@include trans-bg($yellow,.8);
		}
		
		&:before {
			background-color: darken($yellow,40%);
		}
	}
	
	&.dark-blue {
		> span {
			@include trans-bg($dark-blue,.8);
		}
		
		&:before {
			background-color: darken($dark-blue,40%);
		}
	}

     &:before {
		z-index: -1;
		position: absolute;
		top:6%;
		left:0;
		width: 3%;
		height: 100%;
		content: ' ';
		background-color: darken($light-blue,20%);
		-webkit-transform: skewY(40deg);
		-moz-transform: skewY(40deg);
		-o-transform: skewY(40deg);
		transform: skewY(40deg);
		//-webkit-box-shadow: inset 1px -1px 3px #000, 1px 1px 3px #000;
		//-moz-box-shadow: inset 1px -1px 3px #000, 1px 1px 3px #000;
		//box-shadow: inset 1px -1px 3px #000, 1px 1px 3px #000;
	}
}


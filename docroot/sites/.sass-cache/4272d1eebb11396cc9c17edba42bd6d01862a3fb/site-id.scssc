		width: 100%;
		display: block;
		margin: 0 0 1em 0;
		img {
			@include col(1,4,0);
			float: left;
		}
	}
	.body.without-cover-image {
		width: 100%;
		@include multi-col-text(2,2em);
	}
	.body.with-cover-image {
		@include col-last(1,2,5);
		//width: 100%;
	}
	.photo-column {
		@include col(1,2,5);
		img.cover-image {
			@include border-radius($b-radius,$b-radius,$b-radius,$b-radius);
		}
	}
	.img-fill {
			@include border-radius($b-radius,$b-radius,$b-radius,$b-radius);
			position: absolute;
			@include border-radius(0,$b-radius,$b-radius,0);
			width: 43%;
			height: 600px;
			overflow: hidden;
			float: left;
			top: 0;
			left: 57%;
			right: 0;
			bottom: inherit;
			background-size: cover;
			background-repeat: no-repeat;
	}
	
	.explicit-images {
		margin-top: 15px;
		background: black;
		position: relative;
		a.trigger {
			padding: 15px 15px 15px 90px;
			display: block;
			background: $yellow;
			color: $dark-blue;
			font-weight: 800;
			text-transform: uppercase;
			text-align: left;
			background-image: url(../images/warning_black.png);
			background-position: 15px center;
			background-repeat: no-repeat;

			@include border-radius($b-radius,$b-radius,$b-radius,$b-radius);
			
			small {
				display: block;
				font-size: .8em;
				font-weight: 100;
			}
		}
		.explicit-images-content {
			padding: 15px;
			text-align: center;
			max-height: 0px;
			display: none;
			@include transitions();
			//display: none;
			img {
				margin-bottom: 15px;
			}
			&.active {
				display: block;
				max-height: 5000px;
			}
		}
		&:after {
			content: " ";
			width: 32px;
			height: 32px;
			position: absolute;
			margin: 0 48%;
			display: block;
			bottom: -35px;
			left: 0;
			background-image: url(../images/arrows.png);
			background-repeat: no-repeat;
			opacity: .7;
			@include transitions();
			background-position: center -32px;
		}
	}
	
	@include break(v-tablet) { 
		.contents {
			display: block;
		}
		.img-fill {
			@include border-radius($b-radius,$b-radius,$b-radius,$b-radius);
			@include border-radius(0,$b-radius,$b-radius,0);
			position: relative;
			width: 30%;
			float: left;
			left: 0;
			height: 200px;
			margin: 0 1em 1em 0;
		}
		.body {
			width: 100% - ($wrap-pad*2);
			padding: $wrap-pad;
		}
		.body.without-cover-image, .body.with-cover-image {
			width: 100% - ($wrap-pad*2);
			@include multi-col-text(1,0);
		}
		.body.with-cover-image {
			//float: right;
		}
		.photo-column {
			//float: left;
			width: 100%;
		}
	}
}

.event-details {
	overflow: visible;
	position: relative;
	margin-bottom: 3em;
		
	.img-fill, .img-mask {
		@include border-radius(0,$b-radius,0,0);
		display:block;
		width: 50%;
		position:absolute;
		top:0px; right:0px; left:50%; bottom:0px;
		background-repeat:no-repeat;
		background-position:50% 50%;
		background-size:cover;
	}
	.contents {
	}
	
	.img-mask {
		width: 50%;
		top:0px; right:0px; left:50%; bottom:0px;
		background-repeat:no-repeat;
		background-position:50% 50%;
		background-size: cover;
		img {
			width: 100%;
		}
	}

	@include break(tablet) { padding-left: $wrap-pad; }
	
	@include break(tablet) { padding-left: $wrap-pad; }
	
	@include break(v-tablet) { 
		.img-fill, .img-mask {
			position: static;
			height: 300px;
			width: 100%;
			@include border-radius($b-radius,0,0,0);
		}
		.right-column {
			position: relative;
		}
		.left-column {
			padding-top: 15px;
			width: 100% - $wrap-pad;
			
		}
	}
}

.report-details {
	overflow: visible;
	position: relative;
	margin-bottom: 3em;
	h3 {
		text-transform: uppercase;
		font-weight: 100;
	}
	
	ul, address {
		&.details, &.contact-details {
			margin: 0 0 .5em 0; 
		}
	}

	.label {
		padding: 0;
		width: 48%;
	}		
	.img-fill, .img-mask {
		@include border-radius(0,$b-radius,0,0);
		display:block;
		width: 50%;
		position:absolute;
		top:0px; right:0px; left:50%; bottom:350px;
		background-repeat:no-repeat;
		background-position:50% 50%;
		background-size:cover;
	}
	
	@include break(tablet) { padding-left: $wrap-pad; }
	
	@include break(v-tablet) { 
		.img-fill, .img-mask {
			position: static;
			height: 300px;
			width: 100%;
			@include border-radius($b-radius,0,0,0);
		}
		.right-column {
			position: relative;
		}
		.left-column {
			padding-top: 15px;
			width: 100% - $wrap-pad;
			
		}
	}
}

// ! Generic View Element Styling =======================================================

.view-footer {
	.button {
		@include border-radius(0,0,$b-radius,$b-radius);
		&.desk-2-gut-sm {
			&:first-child {
				@include border-radius(0,0,0,$b-radius);
			}
			&:last-child {
				@include border-radius(0,0,$b-radius,0);
			}
		}
 	}
}

// !Stories =======================================================
.gallery-thumb {
	// Most of the width styling is provided inline in the parent document
	float: left; 
	height: 350px; 
	background-size: cover;
	background-position: 50% 50%;
	@include transitions();
	cursor: pointer;
	//-webkit-box-shadow: inset -5px 0px 15px 0px rgba(0, 0, 0, .5);
	//box-shadow: inset -5px 0px 15px 0px rgba(0, 0, 0, .5);
	&:first-child {
		@include border-radius($b-radius,0,0,$b-radius);
	}
	&:last-child {
		@include border-radius(0,$b-radius,$b-radius,0);
	}
	&.active {
		width: 50%; 
	}
	&.inactive {
		
	}
}

// !Resources List  =======================================================

.view-resources {
	.view-group {
		//padding: 35px 0;
		h3 {
			margin-top: 35px;
			margin-bottom: 35px;
			text-transform: uppercase;
			font-weight: 200;
			font-size: 2em;
			border-bottom: 1px solid $yellow;
		}
		
	}
}

.resource {
	margin-top: 15px;
	border-right: 1px solid desaturate($dark-blue,50%);
	
	&.views-row-first {
		.padding {
			padding-left: 0;
		}
	}
	
	img {
		@include outer-glow();
	}
	
	.video-preview {
		position: relative;
		margin-bottom: 15px;
		
		&:after {
			position: absolute;
			content:"";
			padding: 30px;
			display: block;
			right: 10px;
			bottom: 10px;
			//margin-left: -30px;
			background: url(../images/icons-2.png) no-repeat;
			background-position: left top;
		}
		
	}
	
	.padding {

@import "_mixin.scss", "../../../../all/themes/gway_boot/css/_base-grid.scss", "_layout.scss", "_forms.scss", "_navigation.scss", "_icons.scss", "_social-icons.scss";

/* 
	WEB BRAND IDENTITY BUILDING
--------------------------------------------------------------------------------------------------------
	This stylesheet will override the colors from the Gateway Prototype Base. these css sheets 
	consist of coloring, background, typography size and font-family selection options. If you are 
	prototyping your pages first you may not want to turn this on in your .info file until you get to 
	the point of adding styles to pages. Personally I have found it easier to save this section for 
	last so that you can build color background and styling all at one time once you have the 
	majority of your layouts done.
--------------------------------------------------------------------------------------------------------
	Site ID
	These css sheets consist of coloring, background, typography size and font-family selection options
--------------------------------------------------------------------------------------------------------
*/
body {
  font-size: 110%;
  font-family: $f-secondary;
  letter-spacing: .04em;
  font-weight: 300;
  color: darken($light-grey, 10%);
  //background: #1b2031 url(../images/earth-bg2.jpeg) no-repeat 80% -500px;
  background: $dark-blue url(../images/earth-bg2.jpeg) no-repeat left -300px;
  background-size: cover;
  background-attachment: fixed;
  @include break(tablet) {
  	background-size: 100%;
  	background-position: center top !important;
  	background-attachment: fixed !important;
  }
  @include break(mobile) { 
 	 background: $dark-blue url(../images/mobile-bg.jpg) no-repeat 50% 0 !important;
  }
}


a {
  color: #f8f8f8;
  &:visited {
	  
  }
  &:link {
	  
  }
  &:hover, &:active {
  }
}


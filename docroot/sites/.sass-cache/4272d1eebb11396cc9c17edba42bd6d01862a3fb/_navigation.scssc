----------------------------------------------------------------------------------------------------*/
/* This page is for the editing/overridding navigational elements not styled in the Gateway Prototype.
 * I would suggest leaving color/style changes in site-id.css to expedite the ID changes if they come at a later date.
 * Currently the base theme supports Drupal Menus & Superfish with the style set to 'None' on the configure page located at [site-url]/admin/structure/block/manage/superfish/[block #]/configure
----------------------------------------------------------------------------------------------------*/

$border-radius: 6px;

nav {
  &.menu nav.menu {
    margin: 0 0 0 20px; 
  }
  ul a {
    margin: 5px 0;
    padding: 5px 0 0 10px; 
  }
}

.sf-vertical a {
  margin: 5px 0;
  padding: 5px 0 0 10px; 
}

#header-top-full-bg {
	.sf-menu li {
		float: left; 
	}
	
	.menu-top-navigation {
		li {
			margin: 0 1px 0 0;
			
			@include break(v-tablet) {
				&#menu-1102-2, &#menu-1103-2 {
					display: none;
				}
			}
			
			
			&:last-child {
				a {
					@include trans-bg($light-blue,.5);
					position: relative;
					&:after {
						content: " ";
						width: 100%;
						@include trans-bg($light-blue,.5);
						height: 10px;
						position: absolute;
						left: 0;
						bottom: -10px;
					}
					@include break(mobile) { 
						background: none;
						font-weight: bold;
						color: $yellow;
						&:after {
							display: none;
						}
					}
				}
				
			}
			&:last-child a {
				&:after {
					@include border-radius(0,0,$b-radius,$b-radius);
				}
			} 
			
			&:nth-last-child(2) {
				
				a {
				font-weight: bold;
				color: $yellow;
				}
				@include break(mobile) { 
					clear: left;
					
				}
			}
			
			&#menu-1102-2, &#menu-1103-2 {
				a.active {
					background: none;
				}
			}
			
			a {
				padding: 25px 10px 10px 10px;
				color: white;
				//@include transitions();
				&:hover, &.active {
					background: $light-blue;
					&:after {
						background: $light-blue;
					}
				}
				
				@include break(mobile) { 
					padding: 5px 7px;
				}
			}
		}
	}
	
	#block-menu-menu-content-manager {
		float: left;
		position: relative;
		&:before {
			left: 0;
			position: absolute;
			font-weight: bold;
			content: "MANAGE SITE: ";
			top: 5px;
		}
		li {
			font-weight: bold;
			a {
				padding: 25px 10px 10px 10px;
				color: $yellow;
				//@include transitions();
				&:first-child {
					padding-left: 0;
				}
				&:hover, &.active {
					color: $yellow;
				}
			}
		}
		h1 {
			display: none;
		}
	}
	@include break(mobile) { display: none; }
}
header {
	#main-nav {
		@include break(mobile) {
   			width: 100%;
   			float: none;
   			display: none;
   		}
		
		ul {
			line-height: normal;
			margin: 50px 0 0 0;
			@include break(mobile) {
	   			width: 100%;
	   			float: none;
	   			margin: 20px auto 0 auto;
	   			text-align: center;
	   		}
	
			li {
				margin: 0 1px 0 0;
				&.active-trail {
				a {
					color: $light-grey; 
					@include trans-bg($black,.7);
				}
			}
				
				&:first-child a {
		   		@include border-radius($b-radius,0,0,$b-radius);
		   	}
				
				&:last-child a {
		    		@include border-radius(0,$b-radius,$b-radius,0);
		    }
			    
			    @include break(mobile) {
			   		margin: 0 1px 0 0;
			   		display: inline;
	 	   		}
	
				
				a {
					font-size: 1.2em;
					color: #fff;
					padding: 10px 20px;
					@include trans-bg($black,.4);
					text-transform: uppercase; 
					@include transitions();
					&:before {
						content: "BLESS";
						color: $yellow;
						display: block;
						font-size: .6em;
						line-height: .6em;
						padding: 5px 0 0 0;
					}
				
					&:hover { 
						color: $light-grey; 
						@include trans-bg($light-blue,.7);
					}
					
					@include break(mobile) {
						display: inline-block;
						padding: 10px;
						font-size: 1em;
						//width: 100%;
						//@include border-radius($b-radius,$b-radius,$b-radius,$b-radius);
					}
				}
				
				ul a {
				    color: #fff;
				    font-size: 1em;
				    background: #999;
				    width: 100%;
			    
				    &:hover {
				    	background: #444;
				    	color: #fff; 
				    }
				}
			}
		}
	}
}

#secondary-menu {
	width: 100%;
	ul {
		text-align: center; 
	}
	li {
		display: inline;
		
		&.active a, &.active-trail a {
			@include trans-bg(darken($dark-blue, 5%),.8);
			padding: 20px 0;
			@include border-radius($b-radius,$b-radius,0,0);
			font-size: 1.35em;
			&:hover {
				@include trans-bg(darken($dark-blue, 5%),.8);
				cursor:default;
			}
		}
		
		a {
			width: 17%;
			color: #fff;
			font-weight: 100;
			font-size: 1.1em;
			letter-spacing: .05em;
			margin: 0 1px;
			@include col(1,5,0);
			display: inline-block;
			float: none;
			@include trans-bg(darken($yellow, 30%),.8);
			@include border-radius($b-radius,$b-radius,$b-radius,$b-radius);
			text-align: center;
			padding: .5em 0; 
			text-transform: uppercase;
			@include transitions();
			
			&:hover {
				@include trans-bg($light-blue,.6);
			}
			
			&:last-child {
				@include col-last(1,5,0);
				display: inline-block;
				float: none;
			}
		} 
		@include break(mobile) {
			display: block;
			width: 100%;
			a {
				@include col(1,1,0);
				display: block;
				width: 100%;
				margin: 1px 0 0 0;
				@include border-radius(0,0,0,0);
				&:last-child {
					@include col(1,1,0);
				}
			}
			&.active a, &.active-trail a {
				padding: .5em 0;
				@include border-radius(0,0,0,0);
				font-size: 1.1em;
				@include trans-bg($light-blue,.6);
			}
		}
	}
	@include break(mobile) {
		display: none;
	}
}

#mobile-nav {
	display: none;
	/*
	position: absolute;
	top: 0;
	left: 0;
	*/
	width: 100%;
	z-index: 500;
	@include break(mobile) {
		display: block;
	}
	.mobile-nav-wrapper {
		z-index: 500;
		max-height: 0;
		overflow: hidden;
		background-color: #333;
		width: 100%;
		-webkit-box-shadow: inset 0px 0px 10px 0px rgba(0, 0, 0, 1);
		box-shadow: inset 0px 0px 10px 0px rgba(0, 0, 0, 1);
		padding:0;
		@include transitions();
		
		nav.menu {
			margin: 0;
			ul {
				margin: 0;
				li {
					margin: 0;
					border-top: 1px solid #444;
					border-bottom: 1px solid #222;
					text-shadow: 2px 2px 5px #000;
					> a {
						font-size: 1.2em;
						margin: 0;
						padding: 0;
						padding: 5px $wrap-pad;
						&:before {
							content: "BLESS";
							color: $yellow;
							display: block;
							font-size: .6em;
							line-height: .6em;
							padding: 5px 0 0 0;
						}
						&.active {
							color: $light-blue;
						}
					}
					
					> nav.menu {
						background: #222;
						padding: 0;
						-webkit-box-shadow: inset 0px 0px 10px 0px rgba(0, 0, 0, 1);
						box-shadow: inset 0px 0px 10px 0px rgba(0, 0, 0, 1);
						//border-top: 1px solid #444;
						ul {
							//border-top: 1px solid #222;
							padding: 0;
							li {
								border-top: 1px solid #333;
								border-bottom: 1px solid #111;
								padding: 0;
								a {
									padding: 5px $wrap-pad+2%;
									font-size: 1em;
									&:before {
										display: none;
									}
								}
							}
						}
					}
				}	
			}
		}
		
		ul#superfish-2 {
			padding: 0;
			margin: 0;
			list-style-type: none;
			> li {
				@include col(1,2,0);
				float: left;
				list-style-type: none;
				//padding: 5px $wrap-pad;
				margin: 0;
				border-top: 1px solid #444;
				border-bottom: 1px solid #222;
				font-size: .8em;
				text-align: center;
				text-shadow: 2px 2px 5px #000;
				
				&#menu-513-2 {
					display: none;
				}
				
				&.odd {
					a {
						border-right: 1px solid #222;
					}
				}
				&.even {
					a {
						border-left: 1px solid #444;
					}
				}
				a {
					padding: 5px $wrap-pad*2;
					margin: 0;
					font-size: 1em;
					color: #fff;
					//color: $yellow;
					text-transform: uppercase; 
					&:hover, &:active {
						color: $yellow;
					}
				}
				&:hover { 
					color: $light-grey; 
					//@include trans-bg($light-blue,.7);
				}
			}
		}
		
		&.active {
			max-height: 1000px;
			//overflow: auto;
		}
	}
	
	a#mobile-nav-trigger {
		display: none;
		@include break(mobile) {
			display: block;
			z-index: 500;
			background-color: $light-blue;
			text-indent: -9999px;
			background-image: url(../images/mobile-nav-butto.png);
			background-position: center center;
			background-repeat: no-repeat;
			width: 60px;
			height: 40px;
			margin: 0 auto;
			@include border-radius(0,0,$b-radius,$b-radius);
			@include transitions();
			-webkit-box-shadow:  5px 0px 10px 0px rgba(0, 0, 0, .5);
		     box-shadow:  5px 0px 10px 0px rgba(0, 0, 0, .5);
		     margin-bottom: 50px;
			&.active {
				bottom: 300px;
				background-color: #333;
				-webkit-box-shadow: none;
				box-shadow: none;
			}
		}
	}
}

 //@include gradient-trans(darken($dark-blue, 5%),.4,darken($dark-blue, 5%),.8);

#postscript-bottom-full-bg, 
#mobile-nav {
	// Footer Styles
	ul {
		margin: 0;
		padding: 0;
		list-style-type: none;
		li {
			list-style-type: none;
			margin: 0;
			padding: 0;
		}
		&.social {
			li {
				margin: 0 0 20px 0;
				background-position: -60px center;
				background-repeat: no-repeat;
				background-size: auto;
				@include border-radius($b-radius,$b-radius,$b-radius,$b-radius);
				@include transitions();
				display: inline-block;
				@include col(1,2,2);
								
				&:hover, &:active {
					background-position: -40px center;
				}
				
				&.twitter {
					background-image: url(../images/twitter.png);
					//margin-right: 3%;
					@include trans-bg(#3dbbde,0.3);
					&:hover, &:active {
						@include trans-bg(#3dbbde,0.5);
					}
				}
				
				&.facebook {
					@include col-last(1,2,2);
					background-image: url(../images/facebook.png);
					@include trans-bg(#3d60af,0.3);
					&:hover, &:active {
						@include trans-bg(#3d60af,0.5);
					}
				}
				
				a {
					display: block;
					padding: 6% 14px 6% 50px;
					line-height: .8em;
					text-transform: uppercase;
					font-weight: bold;
					@include transitions();
					span {
						font-weight: 100;
						font-size: 70%;
						display: block;
					}
					&:hover, &:active {
						padding-left: 70px;
						color: $yellow;
						span {
							color: white;
						}
					}
				}
			}
		}
	}
}

#mobile-nav {
	@include break(mobile) {
		ul.social {
			margin: 0;
			padding: 0;
			li {
				padding: 0;
				margin: 0;
				//width: 100%;
				@include border-radius(0,0,0,0);
				float: none;
				
				&:first-child {
					margin: 0;
					@include col(1,2,0);
				}
				&:last-child {
					@include col-last(1,2,0);
				}
				
				&.facebook {
					background-image: none;
					background: none;
					@include gradient-trans(#3d60af,.3,#3d60af,1);
					//@include trans-bg(#3d60af,0.5);
				} 
				
				&.twitter {
					background-image: none;
					background: none;
					@include gradient-trans(#3dbbde,.3,#3dbbde,1);
					//@include trans-bg(#3dbbde,0.5);
				}
				a {
					padding: 15px;
					text-align: center;
					&:hover, &:active {
						padding-left: 15px;
						color: $yellow;
						span {
							color: white;
						}
					}
				}
			}
		}	
	}
}

#postscript-bottom-full-bg {
	#block-menu-block-2 {
		border-top: none;
		margin: 0;
		padding: 0;
		li {
			font-size: 1.25em;
			line-height: 1em;
			display: inline-block;
			margin-right: 20px;
			a {
				@include transitions();
				display: inline-block;
				//@include trans-bg($light-blue,.1);
				padding: 0 15px 7px 0;
				border-radius: 3px;
				margin-right: 5px;
				margin: 0;
				display: block;
				text-transform: uppercase;
				&:hover {
					color: $yellow;
					//padding: 0 15px 7px 8px;
				}
				&:before {
					content: "BLESS";
					color: $yellow;
					display: block;
					font-size: .6em;
				}
			}
		}
	}
	#block-menu-menu-top-navigation {
		border-top: none;
		margin-top: 15px;
		padding-top: 0;
		//border: 1px solid red;
		li {
			font-size: .8em;
			line-height: normal;
			display: inline-block;
			background: $yellow;
			margin: 0 5px 8px 0;
			@include border-radius($b-radius,$b-radius,$b-radius,$b-radius);
			@include transitions();
			a {
				@include transitions();
				display: inline-block;
				padding: 5px 10px;
				margin: 0;
				color: $dark-blue;
				text-transform: uppercase;
			}
			&:hover {
				a {
					color: $yellow;
					//padding: 0 15px 15px 8px;
				}
				background: $dark-blue;
			}
		}
		@include break(mobile) {
			display: none;
		}
	}
		
	ul.affiliates {
		//display: none;
		padding: 0;
		margin: 15px 0;
		display: block;
		height: 55px;
		//position: relative;
		li {
			position: absolute;
			left: 0;
			list-style: none;
			margin: 0;
			padding: 0;
			a {
				display: block;
				text-indent: -9999px;
				background-position: left center;
				background-repeat: no-repeat;
				opacity: .15;
				@include transitions();
				&:hover {
					opacity: 1;
				}
				&.gateway-church {
					margin: 0 auto;
					width: 241px;
					height: 48px;
					background-image: url(../images/gateway_logo.png);
				}
			}
		}
	}
	@include break(mobile) {
		display: none;
	}
}
.copyright {
	margin: 0;
}

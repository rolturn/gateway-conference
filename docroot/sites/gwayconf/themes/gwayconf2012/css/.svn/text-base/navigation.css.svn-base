/* TABS
----------------------------------------------------------------------------------------------------*/

.inline,
.tabs,
.primary {
  width: 100%;
}

.inline li,
.tabs li,
.primary li {
  display: inline-block;
  margin: 10px 2px 10px 0;
  padding: 0 ;
  list-style: none;
}

.tabs li,
.primary li {
  margin: 0 2px 10px 0;
}

.inline a,
.tabs a,
.primary a {
  background: #ccc;
  border-radius: 2px;
  color: #FCFBF7;
  display: inline-block;
  font-size: .8em;
  font-weight: bold;
  letter-spacing: .05em;
  padding: 4px 8px 3px;
  text-decoration: none;
  text-transform: uppercase;
}

.inline a:hover,
.tabs a:hover,
.primary a:hover {
  background: #ccc;
  color: #000;
}

.tabs li.active a,
.primary li.active a {
  display: none;
  background: #eee;
  color: #999;
  cursor: auto;
}

.tabs li.active a:hover,
.primary li.active a:hover {
  color: #999;
}


/**
 Aside Menus =======================================================
 */
  
aside nav.item-list ul li,
aside nav.menu ul li {
	background: rgb(210,210,210);
	border-radius: 2px;
	list-style: none;
	margin: 5px 0;
	padding: 5px 0 0 10px;
	
}

aside nav.menu ul li.expanded {
	padding-bottom: 5px;
}
aside nav.menu ul li.expanded a {
	color: rgb(250,250,250);
}

aside nav.menu ul li.expanded nav.menu ul li {
	background: rgb(230,230,230);
	border-radius: 2px 0 0 2px;
}

aside nav.menu ul li.expanded nav.menu ul li a {
	color: rgb(110,110,110);
}

aside nav ul li a {
	border-bottom: none;
	line-height: .8em;
	padding: 10px 0 10px;
}


/**
 More Buttons =======================================================
 */

.more-link {
  text-align: left;
}

.more a,
a.more,
.more-link a,
a.more-link,
.file {
  background-color: #E9E1D3;
  border-radius: 3px;
  color: #8E6E30;
  padding: 5px 8px;
  font-family: 'RokkittRegular', Arial, sans-serif;
}

.more-link a,
a.more-link {
  border-radius: 3px;
  text-transform: uppercase;
  font-size: .7em;
}

aside .more-link a {
  display: block;
  text-align: center;
}

.more a:hover,
a.more:hover,
.more-link a:hover,
a.more-link:hover {
  background-color: #555;
  color: #fff;
  
